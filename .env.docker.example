# Docker Compose Environment Variables
# Copy this file to .env.docker and update the values for your environment

# Application
APP_NAME="NetFlow Analyzer"
APP_ENV=production
APP_DEBUG=false
APP_URL=http://your-server-ip:8003
APP_KEY=

# Application Port
APP_PORT=8003

# Database
DB_DATABASE=netflow_traffic_analyzer
DB_USERNAME=netflow_user
DB_PASSWORD=your_secure_database_password_here
DB_EXTERNAL_PORT=5433

# NetFlow
NETFLOW_PORT=2055
